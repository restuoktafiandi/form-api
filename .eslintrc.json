{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": ["eslint-plugin-import"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "import/no-commonjs": "error",
    "semi": "error"
  }
}
