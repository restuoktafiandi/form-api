{
    "extends": ["@commitlint/config-conventional"],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "test",
          "revert",
          "perf",
          "chore",
          "style",
          "refactor",
          "ci",
          "build"
        ]
      ]
    }
  }